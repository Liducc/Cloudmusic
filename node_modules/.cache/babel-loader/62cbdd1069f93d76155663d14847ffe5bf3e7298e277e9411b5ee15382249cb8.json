{"ast":null,"code":"// 引入axi\nimport axios from 'axios';\nimport { reactive, onMounted } from 'vue';\nexport default {\n  setup() {\n    const state = reactive({\n      images: []\n    });\n    onMountedLazy(() => {\n      try {\n        axios.get('http://localhost:3000/banner?type=2').then(res => {\n          state.images = res.data.banners;\n        });\n      } catch (err) {\n        log;\n      }\n    });\n    return {\n      state\n    };\n  }\n};","map":{"version":3,"names":["axios","reactive","onMounted","setup","state","images","onMountedLazy","get","then","res","data","banners","err","log"],"sources":["E:\\musicdemo\\Cloudmusic\\cloudmusic-app\\src\\components\\home\\SwpierTop.vue"],"sourcesContent":["<template>\r\n    <div id=\"swiperTop\" v-if=\"state.images.length\">\r\n        <van-swipe :autoplay=\"3000\" lazy-render>\r\n            <van-swipe-item v-for=\"image in state.images\" :key=\"image\">\r\n                <img :src=\"image.url\" />\r\n            </van-swipe-item>\r\n        </van-swipe>\r\n    </div>\r\n</template>\r\n<script>\r\n// 引入axi\r\nimport axios from 'axios'\r\nimport { reactive,onMounted } from 'vue';\r\nexport default {\r\n    setup() {\r\n        const state = reactive({\r\n            images:[\r\n        ]});\r\n        onMountedLazy(() => {\r\n            try {\r\n               axios.get('http://localhost:3000/banner?type=2').then((res) => {\r\n                state.images = res.data.banners\r\n            }) \r\n            } catch (err) {\r\n                log\r\n            }\r\n            \r\n        })\r\n        return { state };\r\n    },\r\n};\r\n</script>\r\n<style lang=\"less\">\r\n#swiperTop {\r\n    .van-swipe{\r\n        width: 100%;\r\n        height: 150px;\r\n        .van-swipe-item {\r\n            padding: 0 10px;\r\n            img {\r\n                width: 100%;\r\n                height: 100%;\r\n                border-radius: 10px;\r\n            }\r\n        } \r\n        .van-swipe__indicator--active {\r\n            background-color: red;\r\n        }\r\n\r\n    }\r\n}\r\n</style>"],"mappings":"AAUA;AACA,OAAOA,KAAI,MAAO,OAAM;AACxB,SAASC,QAAQ,EAACC,SAAQ,QAAS,KAAK;AACxC,eAAe;EACXC,KAAKA,CAAA,EAAG;IACJ,MAAMC,KAAI,GAAIH,QAAQ,CAAC;MACnBI,MAAM,EAAC;IACV,CAAC,CAAC;IACHC,aAAa,CAAC,MAAM;MAChB,IAAI;QACDN,KAAK,CAACO,GAAG,CAAC,qCAAqC,CAAC,CAACC,IAAI,CAAEC,GAAG,IAAK;UAC9DL,KAAK,CAACC,MAAK,GAAII,GAAG,CAACC,IAAI,CAACC,OAAM;QAClC,CAAC;MACD,EAAE,OAAOC,GAAG,EAAE;QACVC,GAAE;MACN;IAEJ,CAAC;IACD,OAAO;MAAET;IAAM,CAAC;EACpB;AACJ,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}
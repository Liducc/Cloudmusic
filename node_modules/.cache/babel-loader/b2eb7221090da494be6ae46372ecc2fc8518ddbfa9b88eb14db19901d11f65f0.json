ive, onMounted } from 'vue';\r\nexport default {\r\n    setup() {\r\n        const playList = reactive({\r\n            list: []\r\n        })\r\n\r\n        onMounted(async () => {\r\n            try {\r\n                const res = await getMusicList()\r\n                playList.list = res.data.result\r\n            } catch (error) {\r\n                console.log(error);\r\n            }\r\n        })\r\n        return { playList }\r\n        // axios.get('http://localhost:3000/personalized?limit=30').then((res) => {\r\n        //     console.log(res);\r\n        // })\r\n    }\r\n}\r\n</script>\r\n<style lang=\"less\">\r\n.musicList-top {\r\n    display: flex;\r\n    margin: 5px 15px 10px 15px;\r\n    justify-content: space-between;\r\n    align-items: center;\r\n\r\n    .title {\r\n        font-weight: 900;\r\n    }\r\n\r\n    .more {\r\n        width: 1rem;\r\n        height: .4rem;\r\n        font-size: .2rem;\r\n        border: 1px solid rgb(224, 224, 224);\r\n        border-radius: .2rem;\r\n        display: flex;\r\n        justify-content: center;\r\n        align-items: center;\r\n    }\r\n\r\n    .icon {\r\n        width: .25rem;\r\n        height: .25rem;\r\n    }\r\n}\r\n\r\nimg {\r\n    width: 2rem;\r\n    height: 2rem;\r\n    border-radius: 10%;\r\n    background: rgb(254, 90, 89);\r\n}\r\n\r\n.songList {\r\n    display: flex;\r\n    overflow-x: auto;\r\n}\r\n\r\n.songList::-webkit-scrollbar {\r\n    display: none;\r\n}\r\n\r\n.songList .imgs {\r\n    margin: 18px 0px 0px 18px;\r\n    position: relative;\r\n    .icon{\r\n        width: 0.1rem;\r\n        height: 0.1rem;\r\n    }\r\n}\r\n\r\n.songList p {\r\n    font-size: 5px;\r\n    align-items: center;\r\n    opacity: 0.7;\r\n}\r\n\r\n.count {\r\n    position: absolute;\r\n    right: 0.1rem;\r\n    top: 0.1rem;\r\n    font-size: 0.24rem;\r\n    color: rgb(246, 247, 250);\r\n}\r\n</style>"],"mappings":"AA8BA,SAASA,YAAW,QAAS,uBAAsB;AACnD,SAASC,QAAQ,EAAEC,SAAQ,QAAS,KAAK;AACzC,eAAe;EACXC,KAAKA,CAAA,E